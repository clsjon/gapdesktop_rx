		   xmlns:mx="library://ns.adobe.com/flex/halo" xmlns:ns="library://ns.adobe.com/flex/mx"
		   title="Admin interface" close="titlewindow1_closeHandler(event)"
  xmlns:generated="gapdesktoprx.components.generated.*"
  
  width="600" height="600"
  styleName="plain" xmlns:main="gapdesktoprx.components.main.*">
  <fx:Script>
    <![CDATA[
		import mx.events.CloseEvent;
		import mx.managers.PopUpManager;

		protected function titlewindow1_closeHandler(event:CloseEvent):void
		{
			PopUpManager.removePopUp(this);
		}

     ]]>
  </fx:Script>
	<s:layout><s:VerticalLayout/></s:layout>
	
  <ns:LinkBar dataProvider="{mainViewStack}" direction="vertical" borderStyle="solid" backgroundColor="#EEEEEE"/>
  <ns:ViewStack id="mainViewStack" width="100%" height="100%">
     <generated:ExampleBox/>
    <generated:GraphBox/>
    <generated:SubmitterBox/>
	<generated:TagBox/>
	<generated:ExampleTagBox/> 
	<main:MarshallTest/>
</ns:ViewStack>

