<?xml version="1.0" encoding="utf-8"?>
<ns:Canvas xmlns:fx="http://ns.adobe.com/mxml/2009" 
		   xmlns:s="library://ns.adobe.com/flex/spark" 
		   xmlns:mx="library://ns.adobe.com/flex/halo" xmlns:ns="library://ns.adobe.com/flex/mx" width="400" height="300" creationComplete="canvas1_creationCompleteHandler(event)" label="MarshallTest">
	<fx:Script>
		<![CDATA[
			import gapdesktoprx.models.Graph;
			
			import mx.events.FlexEvent;
			import mx.utils.ObjectUtil;
			
			import org.restfulx.serializers.JSONSerializer;

			protected function canvas1_creationCompleteHandler(event:FlexEvent):void
			{
			var worldGraph:Graph = catalyst.models.graphs[0];
			var ser:JSONSerializer = new JSONSerializer();
			marshallOut.text = ObjectUtil.toString(ser.marshall(worldGraph));
			}

		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<s:TextArea id="marshallOut" width="100%" height="100%" text="marshallOutput"/>
</ns:Canvas>
